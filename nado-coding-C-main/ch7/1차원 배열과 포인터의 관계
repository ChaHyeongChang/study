//ν¬μΈν„° μ—°μ‚°μμ™€  μ¦κ° μ—°μ‚°μλ¥Ό ν•¨κ» μ‚¬μ©ν•λ” μ‹μ€ λΉλ²ν•κ² μ“°μ΄κ³  μ΄μ¤‘μ—μ„ *ptr++μ΄ κ°€μ¥ ν™μ©λ„κ°€ λ†’μ

#include <stdio.h>

int main(void){
    int a[] = {10, 20, 30, 40, 50};
    int i, j = 10;
    int *ptr;

    ptr = a;
    for(i = 0; i < 5; i++){ //*ptrκ³Ό ptr++μ΄ κ²°ν•©λ ν•νƒ
        printf("%d ", *ptr++); //ptr++κ°€ λ¨Όμ € μν–‰λ ν›„ *κ°€ μ μ©λ¨ : *ptr++μ€ ν¬μΈν„°λ¥Ό λ¨Όμ € μ¦κ°€μ‹ν‚¤κ³ , β…μ¦κ°€ν•κΈ° μ „μ μ£Όμ†λ¥Ό λ°ν™ν•¨.β…
    }                            // ptr++ μ¦κ°€ ν•κ³  printfλ¬Έμ΄ μ‹¤ν–‰λ λ•λ” μ¦κ°€ν•κΈ° μ „ ptrμ μ£Όμ†λ¥Ό λ°ν™ λ”°λΌμ„ 10 20 30 40 50 μ¶λ ¥ 
    puts("");

    
    //ex)
        //#include <stdio.h>

        //int main() {
        //    int arr[] = {10, 20, 30};
        //    int *ptr = arr;  // ptrμ΄ arr[0]μ„ κ°€λ¦¬ν‚΄

        //    printf("Before: ptr = %p, *ptr = %d\n", ptr, *ptr); //Before: ptr = 0x600003e40, *ptr = 10 
        //    printf("Value: %d\n", *ptr++); //Value: 10  // *ptrμ„ λ¨Όμ € ν‰κ°€ν• ν›„, ptr μ¦κ°€ 
        //    printf("After: ptr = %p, *ptr = %d\n", ptr, *ptr); //After: ptr = 0x600003e44(4λ°”μ΄νΈμ¦κ°€), *ptr = 20

        //    return 0;
        //}
    


    ptr = a;
    for(i = 0; i < 5; i++){ //++ptrκ³Ό *ptrμ΄ κ²°ν•©λ ν•νƒ
        printf("%d ", *++ptr); //++ptrκ°€ λ¨Όμ € μν–‰λ ν›„ *κ°€ μ μ©λ¨ : *++ptrμ€ ν¬μΈν„°λ¥Ό λ¨Όμ € μ¦κ°€μ‹ν‚¤κ³ , β…μ¦κ°€λ μ£Όμ†λ¥Ό λ°ν™ν•¨.β…
    }                       // ++ptr μ¦κ°€ ν•κ³  printfλ¬Έμ΄ μ‹¤ν–‰λ λ• μ¦κ°€λ ptrμ μ£Όμ†λ¥Ό λ°ν™ λ”°λΌμ„ 20 30 40 50 0 μ¶λ ¥ 
    puts("");


    ptr = a;
    for(i = 0; i < 5; i++){
        printf("%d ", ++*ptr); // ++a[i]μ™€ κ°™μ€ ν•νƒ *ptrμ κ°’μΈ 10μ— ++(*ptr)μ—°μ‚°μ„ ν•΄μ„ 11 12 13 14 15μ κ°’μ΄ λ‚μ΄
    }
    puts("");


    ptr = a;
    for(i = 0; i < 5; i++){
        printf("%d ", (*ptr)++); // a[i]++κ³Ό κ°™μ€ ν•νƒ μ„μ—μ„ *ptrμ κ°’μΈ 15μ— (*ptr)++μ—°μ‚°μ„ ν•΄μ„ 15 16 17 18 19μ κ°’μ΄ λ‚μ΄
    }
    puts("");

    return 0;
}



